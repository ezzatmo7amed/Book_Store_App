<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <navigation-rule>
    <from-view-id>*</from-view-id>
    <navigation-case>
      <from-outcome>login</from-outcome>
      <to-view-id>/Pages/LoginPage.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>registration</from-outcome>
      <to-view-id>/Pages/RegisterationPage.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>index</from-outcome>
      <to-view-id>/index.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/Pages/Registersucces.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>kids</from-outcome>
      <to-view-id>/Pages/Kids.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>fiction</from-outcome>
      <to-view-id>/Pages/Fiction.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>myaccount</from-outcome>
      <to-view-id>/Pages/MyAccount.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>admin</from-outcome>
      <to-view-id>/Pages/AdminPage.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>showemp</from-outcome>
      <to-view-id>/Pages/ShowEmployees.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>AddEmployees</from-outcome>
      <to-view-id>/Pages/AddEmployee.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>newemp</from-outcome>
      <to-view-id>/Pages/NewEmployee.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>cartPage</from-outcome>
      <to-view-id>/Pages/CartPage.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>index2</from-outcome>
      <to-view-id>/index2.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>RegisterBean</managed-bean-name>
    <managed-bean-class>Book.beans.RegisterBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>LoginBean</managed-bean-name>
    <managed-bean-class>Book.beans.LoginBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>StoreBean</managed-bean-name>
    <managed-bean-class>Book.beans.StoreBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>EmployeesBean</managed-bean-name>
    <managed-bean-class>Book.beans.EmployeesBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>EmpBean</managed-bean-name>
    <managed-bean-class>Book.beans.EmpBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</faces-config>